{
  "compilerOptions": {
    "paths": { "@/*": ["./*"] },
    "typeRoots": ["./types", "./node_modules/@types"],
    "baseUrl": "."
  },
  "extends": "expo/tsconfig.base",
  "exclude": ["**/App.js", "**/app/(getStarted)/index.tsx"],
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "app/mealsAndDiet.tsx.backup",
    "app/(questionnaire)/_layout.tsx",
    "types/**/*.d.ts"
  ]
}
